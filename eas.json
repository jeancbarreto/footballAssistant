{
  "cli": {
    "version": ">= 14.2.0", 
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "permissions": [
          "ACCESS_FINE_LOCATION",
          "ACCESS_COARSE_LOCATION",
          "ACCESS_BACKGROUND_LOCATION",
          "FOREGROUND_SERVICE"
        ]
      },
      "env": {
        "GOOGLE_API_KEY": "@GOOGLE_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
